  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-plugin-react": "^7.21.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2"
  },
  "dependencies": {
    "node-sass": "^4.14.1"
  }
}

{
  "name": "prototype",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all build:css build:html",
    "build:css": "node-sass assets/css/style.scss > assets/css/style.css",
    "build:html": "eleventy",
    "watch": "npm-run-all --parallel watch:css watch:html",
    "watch:css": "node-sass --assets/css/style.scss > assets/css/style.css",
    "watch:html": "eleventy --serve --port=8080",
    "start": "npm run watch"
  },
  "repository": {
    "type": "git",
